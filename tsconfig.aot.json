{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./app",
    "paths": {
      "utils": [ "./app/utils" ],
      "core": [ "./app/core" ],
      "pages": [ "./app/pages" ],
      "styles": [ "./app/styles" ],
      "static": [ "./app/static" ],
      "config": [ "./app/config" ],
      "layouts": [ "./app/layouts" ],
      "services": [ "./app/services" ],
      "components": [ "./app/components" ]
    },
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false
  }
}
